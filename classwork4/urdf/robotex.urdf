<?xml version="1.0" ?>
<!--Name of the robot: robotex-->
<robot name="robotex">
    <link name="base_link">
    <visual>
            <geometry>
                <cylinder length="0.5" radius="0.2"/>
            </geometry>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <material name="celeste">
                <color rgba="0 1 1 1"/>
            </material>
        </visual>
    </link>
    <joint name="joint1" type="revolute">
        <parent link="base_link"/>
        <child link="link1"/>
        <origin xyz="0 0 0.1"/>
        <axis xyz="0 0 1" />
        <limit effort="300" velocity="0.1" lower="-3.14" upper="3.14"/>
    </joint>
